// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Match {
  id            Int @id @default(autoincrement())
  title         String
  date          DateTime
  teams         Team[]
}

model Team {
  id      Int @id @default(autoincrement())
  name   String
  matchs  Match[]
}
