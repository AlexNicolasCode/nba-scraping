generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Match {
  id    Int      @id @default(autoincrement())
  title String
  date  DateTime
  teams Team[]   @relation("MatchToTeam")
}

model Team {
  id          Int     @id @default(autoincrement())
  name        String
  acronym     String
  profileLink String
  matchs      Match[] @relation("MatchToTeam")
}
